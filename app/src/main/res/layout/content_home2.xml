<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.cabbooking.rkm.bookmycab.Home2"
    android:background="@drawable/global_home_bg_new"
    tools:showIn="@layout/activity_home2">
    <Button
        android:id="@+id/imagebuttonAddBooking"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:paddingLeft="20dp"
        android:background="@drawable/ic_managebooking"/>
    <Button
        android:id="@+id/imagebuttonbookingHistroy"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:paddingLeft="20dp"
        android:background="@drawable/ic_bookinghistory_1"
        android:layout_toRightOf="@+id/imagebuttonAddBooking"/>

    <Button
        android:id="@+id/imagebuttontaxi"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:paddingLeft="20dp"
        android:background="@drawable/ic_managevehicle"
        android:layout_below="@+id/imagebuttonAddBooking"
       />

    <Button
        android:id="@+id/imageButtonSettings"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:paddingLeft="20dp"
        android:background="@drawable/ic_adduser_new"
        android:layout_toRightOf="@+id/imagebuttonAddBooking"
        android:layout_below="@+id/imagebuttonbookingHistroy"
        />


    <Button
        android:id="@+id/imageButtonAddUser"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:paddingLeft="20dp"
        android:background="@drawable/ic_settings"
        android:layout_below="@+id/imagebuttontaxi"
        android:layout_alignParentStart="true"/>
</RelativeLayout>
