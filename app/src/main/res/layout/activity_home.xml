<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:layout_centerHorizontal="true"
    android:background="@drawable/global_home_bg_new_2"
    tools:context=".HomeActivity">
    <Button
        android:id="@+id/imagebuttonBookingList"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:paddingLeft="20dp"
        android:layout_margin="5dp"
        android:background="@drawable/ic_managebooking"/>

    <Button
        android:id="@+id/imagebuttontaxi"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:paddingLeft="20dp"
        android:layout_margin="5dp"
        android:background="@drawable/ic_managevehicle"
        android:layout_toRightOf="@+id/imagebuttonBookingList"
        />

    <Button
        android:id="@+id/imageButtonAddUser"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:paddingLeft="20dp"
        android:layout_margin="5dp"
        android:background="@drawable/ic_manageuser_new_1"
        android:layout_below="@+id/imagebuttonBookingList"
        />


    <Button
        android:id="@+id/imageButtonSettings"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:paddingLeft="20dp"
        android:background="@drawable/ic_settings"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_alignTop="@+id/imageButtonAddUser"
        android:layout_alignStart="@+id/imagebuttontaxi" />

    <Button
        android:id="@+id/imageButtonTesting"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:paddingLeft="20dp"
        android:text="Test Func"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />
</RelativeLayout>
